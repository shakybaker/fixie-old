<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/site")
    @Styles.Render("~/Content/bootstrap")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/jquery.mCustomScrollbar.css" rel="stylesheet" />
    @RenderSection("Styles", required: false)
    <link href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:400,700,900,400italic' rel='stylesheet' type='text/css'>
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    <script src="/Scripts/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="/Scripts/jquery.signalR-1.0.0-rc1.js"></script>
    <script src="/Scripts/knockout-2.2.0.js"></script>
    <script src="/signalr/hubs"></script>
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/site")

    @RenderSection("Scripts", required: false)
</body>
</html>
